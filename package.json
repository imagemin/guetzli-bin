{
	"name": "guetzli",
	"version": "5.0.0",
	"description": "guetzli wrapper that makes it seamlessly available as a local dependency",
	"license": "MIT",
	"repository": "imagemin/guetzli-bin",
	"type": "module",
	"exports": "./index.js",
	"bin": {
		"guetzli": "cli.js"
	},
	"engines": {
		"node": "^14.13.1 || >=16.0.0"
	},
	"scripts": {
		"postinstall": "node lib/install.js",
		"test": "xo && ava --timeout=60s"
	},
	"files": [
		"index.js",
		"cli.js",
		"lib",
		"vendor/source",
		"test/fixtures"
	],
	"keywords": [
		"imagemin",
		"guetzli",
		"jpeg",
		"jpg",
		"img",
		"image",
		"compress",
		"minify",
		"optimize"
	],
	"dependencies": {
		"bin-build": "^3.0.0",
		"bin-wrapper": "^4.0.0"
	},
	"devDependencies": {
		"ava": "^4.2.0",
		"bin-check": "^4.0.1",
		"compare-size": "^3.0.0",
		"execa": "^6.1.0",
		"executable": "^4.1.0",
		"tempy": "^3.0.0",
		"xo": "^0.48.0"
	}
}
